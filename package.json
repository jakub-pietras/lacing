    "start": "TS_NODE_PROJECT=./examples/tsconfig.json ts-node-dev --require tsconfig-paths/register --respawn ./examples/basic/index.ts"
  },
  "devDependencies": {
    "@types/node": "16.11.21",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.5"
  }
}
