{
  "name": "lacing",
  "description": "Simple server library",
  "version": "0.0.0",
  "author": "Jakub Pietras <jakub.pietras@outlook.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jakub-pietras/lacing.git"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "keywords": [
    "http",
    "node",
    "rest",
    "router",
    "server"
  ],
  "engines": {
    "node": ">= 16.13"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc",
